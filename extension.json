{
  "name"             : "Astrophysics" ,
  "author"           : "Vladimir Lin" ,
  "url"              : "https://github.com/Vladimir-Lin/WikiAstrophysics" ,
  "description"      : "This extension provides some utilities for Astrophysics" ,
  "version"          : "1.00" ,
  "license-name"     : "GPL-2.0+" ,
  "type"             : "others" ,
  "manifest_version" : 1 ,
  "AutoloadClasses"  : { "AstrophysicsWiki": "php/Astronomy.php" },
  "MessagesDirs"     : { "Astrophysics": "i18n" },
  "Hooks"            : { "ParserFirstCallInit": "AstrophysicsWiki::setHooks"} ,
  "ExtensionMessagesFiles": {
    "AstrophysicsExtensionMagic": "Magic.php"
  } ,
  "ResourceModules"  : {
    "ext.Astrophysics"  : {
      "targets" : [ "desktop", "mobile" ],
      "scripts" : [ "Astrophysics.js" ]
    }
  } ,
  "ResourceFileModulePaths": {
    "localBasePath" : "modules",
    "remoteExtPath" : "Astrophysics/modules"
  }
}
